	INNER JOIN RHHealth.dbo.pedidos_exames pe on fp.codigo_pedido_exame = pe.codigo
	INNER JOIN RHHealth.dbo.funcionarios f on pe.codigo_funcionario = f.codigo
where fp.codigo = $P{CODIGO_FICHA_PSICOSSOCIAL}]]>
