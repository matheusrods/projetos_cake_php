	<parameter name="CODIGO_FICHA_ASSISTENCIAL" class="java.lang.String"/>
	<queryString>
		<![CDATA[DECLARE @MaxCount INT;
DECLARE @Cod INT;
SELECT @MaxCount = max(cnt)
FROM (
    SELECT
        count(codigo) AS cnt
    FROM RHHealth.dbo.fichas_assistenciais_questoes fcq
    ) X;
DECLARE @SQL NVARCHAR(max)
    ,@i INT;
SET @i = 0;
WHILE @i < @MaxCount
BEGIN
    SET @i = @i + 1;
    SELECT @Cod = codigo from (
    SELECT codigo, CAST(row_number() OVER (ORDER BY codigo) AS Varchar(10)) AS RowNo FROM RHHealth.dbo.fichas_assistenciais_questoes
    ) pv WHERE RowNo = @i
    SET @SQL = COALESCE(@Sql + ', ', '') + 'questao_' + CONVERT(VARCHAR(10), @Cod);
END
;SET @SQL = N';WITH CTE AS (
        SELECT
        CONCAT(fichas_assistenciais_respostas.resposta COLLATE SQL_Latin1_General_CP1_CI_AS, ''#'' COLLATE SQL_Latin1_General_CP1_CI_AS,
        ISNULL(CASE fichas_assistenciais_respostas.parentesco
        WHEN '''' THEN '' ''
        ELSE RHHealth.publico.ufn_decode_utf8_string(fichas_assistenciais_respostas.parentesco)
        END, '' '') COLLATE SQL_Latin1_General_CP1_CI_AS,
        ''#'' COLLATE SQL_Latin1_General_CP1_CI_AS,
        RHHealth.publico.ufn_decode_utf8_string(fichas_assistenciais_respostas.campo_livre)) COLLATE SQL_Latin1_General_CP1_CI_AS AS resposta,
        ''questao_'' +  CONVERT(VARCHAR(10), fichas_assistenciais_questoes.codigo) COLLATE SQL_Latin1_General_CP1_CI_AS AS RowNo
        FROM RHHealth.dbo.fichas_assistenciais_questoes
        LEFT JOIN RHHealth.dbo.fichas_assistenciais_respostas ON(fichas_assistenciais_respostas.codigo_ficha_assistencial_questao = fichas_assistenciais_questoes.codigo)
        WHERE fichas_assistenciais_questoes.codigo_ficha_assistencial_grupo_questao = 5 AND fichas_assistenciais_respostas.codigo_ficha_assistencial = ' + $P{CODIGO_FICHA_ASSISTENCIAL} + ' )
SELECT *
FROM   CTE
PIVOT (MAX(resposta) FOR RowNo IN (' + @SQL + N')) pvt';
EXECUTE (@SQL)]]>
	</queryString>
