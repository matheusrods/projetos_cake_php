	<queryString>
		<![CDATA[SELECT TOP 1
-- dados da filial
RHHealth.publico.Ufn_decode_utf8_string(clf.razao_social) AS razao_social,
RHHealth.publico.Ufn_decode_utf8_string(clf.nome_fantasia) AS nome_fantasia,
CASE
    WHEN clf.codigo_documento_real IS NULL THEN RHHealth.publico.ufn_formata_cnpj(clf.codigo_documento)
    WHEN clf.codigo_documento_real = '' THEN RHHealth.publico.ufn_formata_cnpj(clf.codigo_documento)
    ELSE RHHealth.publico.ufn_formata_cnpj(clf.codigo_documento_real)
  END AS cnpj,
RHHealth.publico.Ufn_decode_utf8_string(clef.logradouro) AS endereco,
clef.numero AS numero,
RHHealth.publico.Ufn_decode_utf8_string(clef.bairro) AS bairro,
RHHealth.publico.Ufn_decode_utf8_string(clef.cidade) AS cidade,
RHHealth.publico.Ufn_decode_utf8_string(clef.estado_descricao) AS estado,
clf.cnae,
ISNULL(RHHealth.publico.Ufn_decode_utf8_string(cnae.descricao),'') AS ramo_atividade,
ISNULL(cnae.grau_risco, '') AS grau_risco,
ISNULL(RHHealth.publico.Ufn_decode_utf8_string(me.nome), '') AS medico,
ISNULL(me.numero_conselho, '') AS numero_conselho,
ISNULL(me.conselho_uf, '') AS conselho_uf,
ISNULL(RHHealth.publico.Ufn_decode_utf8_string(cp.descricao), '') AS descricao_conselho_profissional,
ISNULL(RHHealth.publico.Ufn_decode_utf8_string(me.especialidade), '') AS especialidade,
CONVERT(varchar(10), gpra.data_inicio_vigencia, 111) AS data_inicio_vigencia,

--pegar os dados da empresa que deve ser impressa os dados como pai
RHHealth.publico.Ufn_decode_utf8_string(cpai.razao_social) as pai_razao_social,
RHHealth.publico.Ufn_decode_utf8_string(cpai.nome_fantasia) as pai_nome_fantasia,
RHHealth.publico.ufn_formata_cnpj(cpai.codigo_documento) as pai_cnpj,
CASE
	WHEN clf.codigo_documento_real IS NULL THEN RHHealth.publico.Ufn_decode_utf8_string(clef.logradouro)
	WHEN clf.codigo_documento_real = '' THEN RHHealth.publico.Ufn_decode_utf8_string(clef.logradouro) -- filho
    ELSE RHHealth.publico.Ufn_decode_utf8_string(clem.logradouro) -- pai
END AS pai_endereco,
CASE
	WHEN clf.codigo_documento_real IS NULL THEN clef.numero
	WHEN clf.codigo_documento_real = '' THEN clef.numero -- filho
    ELSE clem.numero  -- pai
END AS pai_numero,
CASE
	WHEN clf.codigo_documento_real IS NULL THEN RHHealth.publico.Ufn_decode_utf8_string(clef.bairro)
	WHEN clf.codigo_documento_real = '' THEN RHHealth.publico.Ufn_decode_utf8_string(clef.bairro) -- filho
    ELSE RHHealth.publico.Ufn_decode_utf8_string(clem.bairro)   -- pai
END AS pai_bairro,
CASE
	WHEN clf.codigo_documento_real IS NULL THEN RHHealth.publico.Ufn_decode_utf8_string(clef.cidade)
	WHEN clf.codigo_documento_real = '' THEN RHHealth.publico.Ufn_decode_utf8_string(clef.cidade) -- filho
    ELSE RHHealth.publico.Ufn_decode_utf8_string(clem.cidade) -- pai
END AS pai_cidade,
CASE
	WHEN clf.codigo_documento_real IS NULL THEN RHHealth.publico.Ufn_decode_utf8_string(clef.estado_descricao)
	WHEN clf.codigo_documento_real = '' THEN RHHealth.publico.Ufn_decode_utf8_string(clef.estado_descricao) -- filho
    ELSE RHHealth.publico.Ufn_decode_utf8_string(clem.estado_descricao) -- pai
END AS pai_estado
FROM RHHealth.dbo.ppra_versoes pv
	INNER JOIN RHHealth.dbo.grupos_economicos_clientes gec ON gec.codigo_cliente = pv.codigo_cliente_alocacao
	INNER JOIN RHHealth.dbo.cliente clf				ON (clf.codigo = gec.codigo_cliente)
	LEFT JOIN RHHealth.dbo.cnae						ON (cnae.cnae = clf.cnae)
    LEFT JOIN RHHealth.dbo.cliente_endereco clef	ON (clef.codigo_cliente = clf.codigo)
	LEFT JOIN RHHealth.dbo.grupos_prevencao_riscos_ambientais_versoes gpra  ON(gpra.codigo_cliente = gec.codigo_cliente AND gpra.codigo_ppra_versoes = pv.codigo)
	LEFT JOIN RHHealth.dbo.medicos me		        ON(me.codigo = gpra.codigo_medico)
	LEFT JOIN RHHealth.dbo.conselho_profissional cp ON(cp.codigo = me.codigo_conselho_profissional)
	LEFT JOIN RHHealth.dbo.cliente cpai				ON clf.codigo_documento_real = cpai.codigo_documento
	LEFT JOIN RHHealth.dbo.cliente_endereco clem    ON (clem.codigo_cliente = cpai.codigo)
WHERE pv.codigo = $P{CODIGO_PPRA_VERSOES};]]>
