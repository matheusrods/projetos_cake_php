					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font size="8" isBold="true"/>
					</textElement>
					<text><![CDATA[APRECIAÇÃO DAS EXPOSIÇÃO RESULTANTE E POSSÍVEIS AÇÕES]]></text>
				</staticText>
				<line>
					<reportElement x="14" y="430" width="524" height="1" uuid="e2cef7a9-4326-459f-8175-3c0c5dc7c285"/>
