	<queryString>
		<![CDATA[DECLARE @MaxCount INT;
                    DECLARE @Cod INT;
                    SELECT @MaxCount = max(cnt)
                    FROM(
                            SELECT count(codigo) AS cnt
                            FROM RHHealth.dbo.fichas_clinicas_questoes fcq
                    ) X;
                    DECLARE @SQL NVARCHAR(max)
                                        ,@i INT;
                    SET @i = 0;
                    WHILE @i < @MaxCount
                    BEGIN
                        SET @i = @i + 1;
                        SELECT @Cod = codigo from (
                            SELECT codigo, CAST(row_number() OVER (ORDER BY codigo) AS Varchar(10)) AS RowNo
                            FROM RHHealth.dbo.fichas_clinicas_questoes
                        ) pv WHERE RowNo = @i
                        SET @SQL = COALESCE(@Sql + ', ', '') + 'questao_' + CONVERT(VARCHAR(10), @Cod);
                    END
                    ;SET @SQL = N';WITH CTE AS
                    (
                    SELECT
                        CONCAT(
                            IIF(Respostas.resposta  = ''0'', '' '', dbBuonny.publico.ufn_decode_utf8_string(Respostas.resposta)) COLLATE SQL_Latin1_General_CP1_CI_AS,
                            ''#'' COLLATE SQL_Latin1_General_CP1_CI_AS,
                            ISNULL(CASE Respostas.parentesco
                            WHEN '''' THEN '' ''
                            ELSE dbBuonny.publico.ufn_decode_utf8_string(Respostas.parentesco)
                            END, '' '') COLLATE SQL_Latin1_General_CP1_CI_AS,
                            ''#'' COLLATE SQL_Latin1_General_CP1_CI_AS,
                            CASE Respostas.campo_livre
                            WHEN '''' THEN '' ''
                            ELSE dbBuonny.publico.ufn_decode_utf8_string(Respostas.campo_livre)
                            END COLLATE SQL_Latin1_General_CP1_CI_AS) AS resposta,
                            ''questao_'' +  CONVERT(VARCHAR(10), Questoes.codigo) COLLATE database_default AS RowNo,
                            dbBuonny.publico.ufn_decode_utf8_string(Cliente.nome_fantasia) AS cliente_nome_fantasia,
                            dbBuonny.publico.ufn_decode_utf8_string(Cliente.codigo_documento) AS cliente_cnpj,
                            dbBuonny.publico.ufn_decode_utf8_string(Funcionario.nome) AS funcionario_nome,
                            dbBuonny.publico.ufn_decode_utf8_string(Funcionario.cpf) AS funcionario_cpf,
							(
								SELECT
									COUNT(Resp.resposta)
								FROM RHHealth.dbo.fichas_clinicas_respostas AS Resp
								WHERE Resp.codigo_ficha_clinica = ' + $P{CODIGO_FICHA_CLINICA} + ' AND
									codigo_ficha_clinica_questao IN (293,294,295,296,297,298) AND
									Resp.resposta IS NOT NULL
							) AS Count_Def
                            FROM RHHealth.dbo.fichas_clinicas_questoes AS Questoes
                            LEFT JOIN RHHealth.dbo.fichas_clinicas_respostas AS Respostas
                            ON (Respostas.codigo_ficha_clinica_questao = Questoes.codigo)
                            LEFT JOIN RHHealth.dbo.fichas_clinicas AS FichaClinica
                            ON (Respostas.codigo_ficha_clinica = FichaClinica.codigo)
                            LEFT JOIN RHHealth.dbo.pedidos_exames AS PedidoExame
                            ON FichaClinica.codigo_pedido_exame = PedidoExame.codigo
                            LEFT JOIN RHHealth.dbo.cliente AS Cliente
                            ON PedidoExame.codigo_cliente = Cliente.codigo
                            LEFT JOIN RHHealth.dbo.funcionarios AS Funcionario
                            ON PedidoExame.codigo_funcionario = Funcionario.codigo
                            WHERE Questoes.codigo_ficha_clinica_grupo_questao = 8 AND Respostas.codigo_ficha_clinica = ' + $P{CODIGO_FICHA_CLINICA} + '
                        )
                    SELECT *
                    FROM   CTE
                    PIVOT (MAX(resposta) FOR RowNo IN (' + @SQL + N')) pvt';
                    EXECUTE (@SQL)]]>
	</queryString>
