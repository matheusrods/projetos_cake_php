	<queryString>
		<![CDATA[SELECT
    dbBuonny.publico.ufn_decode_utf8_string(cnae.descricao) COLLATE SQL_Latin1_General_CP1_CI_AS AS ramo_atividade,
    ISNULL(cnae.grau_risco, '') COLLATE SQL_Latin1_General_CP1_CI_AS AS grau_risco,
    CONVERT(VARCHAR(10), pv.inicio_vigencia_pcmso, 103) COLLATE SQL_Latin1_General_CP1_CI_AS AS inicio_vigencia_pcmso,
    CONVERT(VARCHAR(10), DATEADD(month, CONVERT(int, pv.periodo_vigencia_pcmso), pv.inicio_vigencia_pcmso), 103) COLLATE SQL_Latin1_General_CP1_CI_AS AS fim_vigencia_pcmso,
    pv.periodo_vigencia_pcmso AS vigencia_em_meses,
    -- endereco da matriz
    dbBuonny.publico.ufn_decode_utf8_string(clf.razao_social) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_razao_social,
    dbBuonny.publico.ufn_decode_utf8_string(clf.nome_fantasia) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_nome_fantasia,
    dbBuonny.publico.ufn_formata_cnpj(CASE WHEN(clf.codigo_documento_real IS NOT NULL AND clf.codigo_documento_real <> '' ) THEN
                                        clf.codigo_documento_real COLLATE SQL_Latin1_General_CP1_CI_AS
                                      ELSE
                                        clf.codigo_documento COLLATE SQL_Latin1_General_CP1_CI_AS
                                      END) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_codigo_documento,
    me.nome as nome_medico,
    me.numero_conselho as numero_conselho,
    me.conselho_uf as conselho_uf,
    clm.cnae COLLATE SQL_Latin1_General_CP1_CI_AS AS cnae,
    ----------------
    -- endereco da filial
    dbbuonny.publico.Ufn_decode_utf8_string(clef.logradouro) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_endereco,
    ISNULL(dbbuonny.publico.Ufn_decode_utf8_string(clef.complemento),'') COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_complemento,
    clef.numero AS filial_numero,
    dbbuonny.publico.Ufn_decode_utf8_string(clef.bairro) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_bairro,
    dbbuonny.publico.Ufn_decode_utf8_string(clef.cidade) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_cidade,
    CONCAT(' ' COLLATE SQL_Latin1_General_CP1_CI_AS, dbbuonny.publico.Ufn_decode_utf8_string(clef.estado_descricao) COLLATE SQL_Latin1_General_CP1_CI_AS) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_estado,
    clef.cep COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_cep,
    -----------------
    (
        SELECT count(sfu.codigo) AS count
            FROM RHHealth.dbo.cliente_funcionario scf
        INNER JOIN RHHealth.dbo.funcionarios sfu ON (sfu.codigo = scf.codigo_funcionario)
        INNER JOIN RHHealth.dbo.funcionario_setores_cargos fscc ON(fscc.codigo_cliente_funcionario = scf.codigo)
	    	WHERE fscc.codigo_cliente = $P{CODIGO_CLIENTE}
        AND (FLOOR(DATEDIFF(DAY, sfu.data_nascimento, getdate()) / 365.25) < 18)
    ) AS quant_menor_idade,
    (
        SELECT count(sfu.codigo) AS count
            FROM RHHealth.dbo.cliente_funcionario scf
        INNER JOIN RHHealth.dbo.funcionarios sfu ON (sfu.codigo = scf.codigo_funcionario)
        INNER JOIN RHHealth.dbo.funcionario_setores_cargos fscc ON(fscc.codigo_cliente_funcionario = scf.codigo)
	    	WHERE fscc.codigo_cliente = $P{CODIGO_CLIENTE}
        AND sfu.sexo = 'F'
    ) AS quant_funcionario_feminino,
    (
        SELECT count(sfu.codigo) AS count
            FROM RHHealth.dbo.cliente_funcionario scf
        INNER JOIN RHHealth.dbo.funcionarios sfu ON (sfu.codigo = scf.codigo_funcionario)
        INNER JOIN RHHealth.dbo.funcionario_setores_cargos fscc ON(fscc.codigo_cliente_funcionario = scf.codigo)
    		WHERE fscc.codigo_cliente = $P{CODIGO_CLIENTE}
        AND sfu.sexo = 'M'
    ) AS quant_funcionario_masculino
-----------------
FROM RHHealth.dbo.grupos_economicos_clientes gec
    INNER JOIN RHHealth.dbo.grupos_economicos ge ON (ge.codigo = gec.codigo_grupo_economico)
    --cliente matriz
    INNER JOIN RHHealth.dbo.cliente clm ON (clm.codigo = ge.codigo_cliente)
    -- cliente filial
    INNER JOIN RHHealth.dbo.cliente clf ON (clf.codigo = gec.codigo_cliente)
    LEFT JOIN RHHealth.dbo.cnae ON (cnae.cnae = clm.cnae)
    -- obtem o endereco da filial
    LEFT JOIN RHHealth.dbo.cliente_endereco clef ON (clef.codigo_cliente = clf.codigo)
    LEFT JOIN RHHealth.dbo.medicos me ON (me.codigo = ISNULL(clf.codigo_medico_pcmso,clm.codigo_medico_pcmso))
    LEFT JOIN RHHealth.dbo.ordem_servico os ON(os.codigo_grupo_economico = ge.codigo AND
                                               os.codigo_cliente = gec.codigo_cliente AND
                                               os.codigo IN (SELECT codigo_ordem_servico
                                                                FROM RHHealth.dbo.ordem_servico_item
                                                             WHERE codigo_servico=2340)
                                               )
	INNER JOIN RHHealth.dbo.pcmso_versoes pv  ON (gec.codigo_cliente = pv.codigo_cliente_alocacao)
WHERE gec.codigo_cliente = $P{CODIGO_CLIENTE} AND
	pv.codigo = $P{CODIGO_PCMSO_VERSAO}]]>
	</queryString>
