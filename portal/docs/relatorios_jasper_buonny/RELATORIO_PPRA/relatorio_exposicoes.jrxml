	<parameter name="CODIGO_GRUPO_EXPOSICAO" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
dbBuonny.publico.ufn_decode_utf8_string(ISNULL((
SELECT grupos_riscos.descricao FROM RHHealth.dbo.grupos_riscos
    WHERE grupos_riscos.codigo = ri.codigo_grupo
), '')) AS agente,
    dbBuonny.publico.ufn_decode_utf8_string(ri.nome_agente) AS risco,
    ISNULL(radec.descricao, '') AS efeito_critico,
    ISNULL(radmp.descricao, '') AS meio_propagacao,
    ISNULL(ecate.abreviacao, '') as tempo_exposicao_tipo,
    ISNULL(ger.minutos_tempo_exposicao, '') AS tempo_exposicao_minutos,
    ISNULL(ger.jornada_tempo_exposicao, '') AS tempo_exposicao_jornada,
    ger.codigo AS CODIGO_GRUPOS_EXPOSICAO_RISCO,
    ISNULL(dbBuonny.publico.ufn_decode_utf8_string(ge.observacao), '') AS observacao,
    ISNULL(dbBuonny.publico.ufn_decode_utf8_string(ger.medidas_controle), '') AS medidas_controle ,
    ISNULL(ecain.abreviacao, '') AS intensidade,
    ISNULL(ecaer.abreviacao, '') AS exposicao_resultante,
    ISNULL(ecapd.abreviacao, '') AS potencial_dano,
    ISNULL(ecagr.abreviacao, '') AS grau_risco,
    CASE WHEN ge.codigo_grupo_homogeneo IS NOT NULL THEN 1
         ELSE 0
         END as grupo_homogeneo,
    CASE WHEN ger.codigo_tipo_medicao = 1 THEN 'Quantitativo'
         WHEN ger.codigo_tipo_medicao = 2 THEN 'Qualitativo'
         WHEN ger.codigo_tipo_medicao is null THEN ''
    END AS tipo_medicao,
    ISNULL(tm.abreviacao, '') AS tecnica_medicao,
    ISNULL(ger.valor_maximo, '') AS limite_tolerancia,
    ISNULL(ger.valor_medido, '') AS valor_medido,
    ISNULL(dbBuonny.publico.ufn_decode_utf8_string(ger.medidas_controle_recomendada), '') as medidas_controle_recomendada
FROM RHHealth.dbo.clientes_setores cs
    INNER JOIN RHHealth.dbo.grupo_exposicao ge
        ON (ge.codigo_cliente_setor = cs.codigo)
    INNER JOIN RHHealth.dbo.grupos_exposicao_risco ger
        ON (ger.codigo_grupo_exposicao = ge.codigo)
    INNER JOIN  RHHealth.dbo.riscos ri
        ON (ri.codigo = ger.codigo_risco)
    LEFT JOIN RHHealth.dbo.riscos_atributos_detalhes radec
        ON (radec.codigo = ger.codigo_efeito_critico)
    LEFT JOIN RHHealth.dbo.riscos_atributos_detalhes radmp
        ON (radmp.codigo = ger.codigo_risco_atributo)
    LEFT JOIN RHHealth.dbo.exposicao_ocupacional_atributo ecate
        ON (ecate.codigo = ger.tempo_exposicao)
    LEFT JOIN RHHealth.dbo.exposicao_ocupacional_atributo ecain
        ON (ecain.codigo = ger.intensidade)
    LEFT JOIN RHHealth.dbo.exposicao_ocupacional_atributo ecaer
        ON (ecaer.codigo = ger.resultante)
    LEFT JOIN RHHealth.dbo.exposicao_ocupacional_atributo ecapd
        ON (ecapd.codigo = ger.dano)
    LEFT JOIN RHHealth.dbo.exposicao_ocupacional_atributo ecagr
        ON (ecagr.codigo = ger.grau_risco)
    LEFT JOIN RHHealth.dbo.tecnicas_medicao tm
        ON (tm.codigo = ger.codigo_tecnica_medicao)
WHERE ge.codigo = $P{CODIGO_GRUPO_EXPOSICAO}]]>
	</queryString>
