	<queryString>
		<![CDATA[DECLARE @MaxCount INT;
DECLARE @Cod INT;
SELECT @MaxCount = max(cnt)
FROM (
    SELECT
        count(codigo) AS cnt
    FROM RHHealth.dbo.fichas_clinicas_questoes fcq
    ) X;
DECLARE @SQL NVARCHAR(max)
    ,@i INT;
SET @i = 0;
WHILE @i < @MaxCount
BEGIN
    SET @i = @i + 1;
    SELECT @Cod = codigo from (
    SELECT codigo, CAST(row_number() OVER (ORDER BY codigo) AS Varchar(10)) AS RowNo FROM RHHealth.dbo.fichas_clinicas_questoes
    ) pv WHERE RowNo = @i
    SET @SQL = COALESCE(@Sql + ', ', '') + 'questao_' + CONVERT(VARCHAR(10), @Cod);
END
;SET @SQL = N';WITH CTE AS (
        SELECT
        CONCAT(dbBuonny.publico.ufn_decode_utf8_string(fichas_clinicas_respostas.resposta) COLLATE SQL_Latin1_General_CP1_CI_AS , ''#'' COLLATE SQL_Latin1_General_CP1_CI_AS,
                ISNULL(CASE fichas_clinicas_respostas.parentesco
                WHEN '''' THEN '' ''
                    ELSE dbBuonny.publico.ufn_decode_utf8_string(fichas_clinicas_respostas.parentesco)
                END, '' '') COLLATE SQL_Latin1_General_CP1_CI_AS,
        ''#'' COLLATE SQL_Latin1_General_CP1_CI_AS,
        dbBuonny.publico.ufn_decode_utf8_string(fichas_clinicas_respostas.campo_livre)) COLLATE SQL_Latin1_General_CP1_CI_AS AS resposta,
        ''questao_'' +  CONVERT(VARCHAR(10), fichas_clinicas_questoes.codigo) COLLATE SQL_Latin1_General_CP1_CI_AS AS RowNo
        FROM RHHealth.dbo.fichas_clinicas_questoes
        LEFT JOIN RHHealth.dbo.fichas_clinicas_respostas ON (fichas_clinicas_respostas.codigo_ficha_clinica_questao = fichas_clinicas_questoes.codigo)
        WHERE fichas_clinicas_questoes.codigo_ficha_clinica_grupo_questao = 3
            AND fichas_clinicas_respostas.codigo_ficha_clinica = '+$P{CODIGO_FICHA_CLINICA}+' )
SELECT *
FROM   CTE
PIVOT (MAX(resposta) FOR RowNo IN (' + @SQL + N')) pvt';
EXECUTE (@SQL)]]>
	</queryString>
