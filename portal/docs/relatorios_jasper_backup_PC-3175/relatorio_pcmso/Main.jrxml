	<queryString>
		<![CDATA[-- pcmso

SELECT TOP(1)
    RHHealth.publico.Ufn_decode_utf8_string(cnae.descricao) COLLATE SQL_Latin1_General_CP1_CI_AS AS ramo_atividade,
    ISNULL(cnae.grau_risco, '') COLLATE SQL_Latin1_General_CP1_CI_AS AS grau_risco,
    CONVERT(VARCHAR(10), os.inicio_vigencia_pcmso, 103) COLLATE SQL_Latin1_General_CP1_CI_AS AS inicio_vigencia_pcmso,
    CONVERT(VARCHAR(10), DATEADD(DAY, -1,DATEADD(MONTH, CONVERT(INT, os.vigencia_em_meses), os.inicio_vigencia_pcmso)), 103) COLLATE SQL_Latin1_General_CP1_CI_AS AS fim_vigencia_pcmso,
    os.vigencia_em_meses,
    me.nome as nome_medico,
    me.numero_conselho as numero_conselho,
    me.conselho_uf as conselho_uf,
	-- dados matriz
	(case when clm.razao_social is null then RHHealth.publico.Ufn_decode_utf8_string(clf.razao_social) else RHHealth.publico.Ufn_decode_utf8_string(clm.razao_social) end) as matriz_razao_social,
	(case when clm.codigo_documento is null then isnull(clf.codigo_documento,clf.codigo_documento_real) else isnull(clm.codigo_documento_real,clm.codigo_documento)  end) as matriz_codigo_documento,

	(case when cmef.logradouro is null then RHHealth.publico.Ufn_decode_utf8_string(clef.logradouro) else RHHealth.publico.Ufn_decode_utf8_string(cmef.logradouro) end) as matriz_endereco,
    (case when cmef.numero is null then clef.numero else cmef.numero end) as matriz_numero,
    (case when cmef.complemento is null then RHHealth.publico.Ufn_decode_utf8_string(clef.complemento) else RHHealth.publico.Ufn_decode_utf8_string(cmef.complemento) end) as matriz_complemento,
    (case when cmef.cep is null then clef.cep else cmef.cep end) as matriz_cep,
    (case when cmef.cidade is null then RHHealth.publico.Ufn_decode_utf8_string(clef.cidade) else RHHealth.publico.Ufn_decode_utf8_string(cmef.cidade) end) as matriz_cidade,
    (case when cmef.bairro is null then RHHealth.publico.Ufn_decode_utf8_string(clef.bairro) else RHHealth.publico.Ufn_decode_utf8_string(cmef.bairro) end) as matriz_bairro,
    (case when cmef.estado_abreviacao is null then clef.estado_abreviacao else cmef.estado_abreviacao end) as matriz_estado,
	(case when clm.cnae is null then clf.cnae else cnaem.cnae end) as matriz_cnae,
	(case when cnaem.grau_risco is null then ISNULL(cnae.grau_risco, '') else ISNULL(cnaem.grau_risco, '') end) as matriz_cnae_grau_risco,
    (case when cnaem.descricao is null then RHHealth.publico.Ufn_decode_utf8_string(cnae.descricao) else RHHealth.publico.Ufn_decode_utf8_string(cnaem.descricao) end) as matriz_cnae_descricao,
	-- filial se é tomador
    clf.e_tomador as filial_e_tomador,
    -- endereco da matriz
    RHHealth.publico.Ufn_decode_utf8_string(clf.razao_social) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_razao_social,
    RHHealth.publico.Ufn_decode_utf8_string(clf.nome_fantasia) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_nome_fantasia,
    RHHealth.publico.ufn_formata_cnpj(CASE WHEN(clf.codigo_documento_real IS NOT NULL AND clf.codigo_documento_real <> '' ) THEN
                                        clf.codigo_documento_real COLLATE SQL_Latin1_General_CP1_CI_AS
                                      ELSE
                                        clf.codigo_documento COLLATE SQL_Latin1_General_CP1_CI_AS
                                      END) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_codigo_documento,
    clf.cnae COLLATE SQL_Latin1_General_CP1_CI_AS AS cnae,
    -- endereco da filial
    RHHealth.publico.Ufn_decode_utf8_string(clef.logradouro) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_endereco,
    ISNULL(RHHealth.publico.Ufn_decode_utf8_string(clef.complemento),'') COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_complemento,
    clef.numero AS filial_numero,
    RHHealth.publico.Ufn_decode_utf8_string(clef.bairro) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_bairro,
    RHHealth.publico.Ufn_decode_utf8_string(clef.cidade) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_cidade,
    CONCAT(' ' COLLATE SQL_Latin1_General_CP1_CI_AS, RHHealth.publico.Ufn_decode_utf8_string(clef.estado_descricao) COLLATE SQL_Latin1_General_CP1_CI_AS) COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_estado,
    clef.cep COLLATE SQL_Latin1_General_CP1_CI_AS AS filial_cep,
-----------------
  (
        SELECT count(sfu.codigo) AS count
        FROM RHHealth.dbo.cliente_funcionario scf
        INNER JOIN RHHealth.dbo.funcionarios sfu ON (sfu.codigo = scf.codigo_funcionario)
        INNER JOIN RHHealth.dbo.funcionario_setores_cargos fsc ON fsc.codigo = (Select top 1 codigo from RHHealth.dbo.funcionario_setores_cargos where codigo_cliente_funcionario = scf.codigo order by 1 desc )
        WHERE fsc.codigo_cliente_alocacao = $P{CODIGO_CLIENTE}
        AND (FLOOR(DATEDIFF(DAY, sfu.data_nascimento, getdate()) / 365.25) < 18)
		AND scf.ativo NOT IN (0)
    ) AS quant_menor_idade,
    (
        SELECT count(sfu.codigo) AS count
        FROM RHHealth.dbo.cliente_funcionario scf
        INNER JOIN RHHealth.dbo.funcionarios sfu ON (sfu.codigo = scf.codigo_funcionario)
        INNER JOIN RHHealth.dbo.funcionario_setores_cargos fsc ON fsc.codigo = (Select top 1 codigo from RHHealth.dbo.funcionario_setores_cargos where codigo_cliente_funcionario = scf.codigo order by 1 desc )
        WHERE fsc.codigo_cliente_alocacao = $P{CODIGO_CLIENTE}
        AND sfu.sexo = 'F'
        AND scf.ativo NOT IN (0)
    ) AS quant_funcionario_feminino,
    (
        SELECT count(sfu.codigo) AS count
        FROM RHHealth.dbo.cliente_funcionario scf
        INNER JOIN RHHealth.dbo.funcionarios sfu ON (sfu.codigo = scf.codigo_funcionario)
        INNER JOIN RHHealth.dbo.funcionario_setores_cargos fsc ON fsc.codigo = (Select top 1 codigo from RHHealth.dbo.funcionario_setores_cargos where codigo_cliente_funcionario = scf.codigo order by 1 desc )
        WHERE fsc.codigo_cliente_alocacao = $P{CODIGO_CLIENTE}
        AND sfu.sexo = 'M'
        AND scf.ativo NOT IN (0)
    ) AS quant_funcionario_masculino
FROM RHHealth.dbo.grupos_economicos_clientes gec
INNER JOIN RHHealth.dbo.grupos_economicos ge ON (ge.codigo = gec.codigo_grupo_economico)

left join RHHealth.dbo.funcionario_setores_cargos fsc on fsc.codigo = (select top 1 codigo from RHHealth.dbo.funcionario_setores_cargos where codigo_cliente_alocacao = gec.codigo_cliente order by codigo desc)

    --cliente matriz
left JOIN RHHealth.dbo.cliente clm ON (clm.codigo = fsc.codigo_cliente_referencia ) -- ge.codigo_cliente)
    -- cliente filial
INNER JOIN RHHealth.dbo.cliente clf ON (clf.codigo = gec.codigo_cliente)
LEFT JOIN RHHealth.dbo.cnae ON (cnae.cnae = clf.cnae)
    -- obtem o endereco da filial
LEFT JOIN RHHealth.dbo.cliente_endereco clef ON (clef.codigo_cliente = clf.codigo)
-- obtem enderedo da matriz
LEFT JOIN RHHealth.dbo.cliente_endereco cmef ON (cmef.codigo_cliente = clm.codigo)
-- obtem cnae matriz
LEFT JOIN RHHealth.dbo.cnae cnaem ON(cnaem.cnae = clm.cnae)
LEFT JOIN RHHealth.dbo.medicos me ON (me.codigo = ISNULL(clf.codigo_medico_pcmso,clm.codigo_medico_pcmso))
LEFT JOIN RHHealth.dbo.ordem_servico os ON(os.codigo_grupo_economico = ge.codigo AND os.codigo_cliente = gec.codigo_cliente AND os.codigo IN (SELECT codigo_ordem_servico FROM RHHealth.dbo.ordem_servico_item WHERE codigo_servico=2340))
WHERE gec.codigo_cliente = $P{CODIGO_CLIENTE}]]>
	</queryString>
